package oopIntro;

public class Product {
	
	public Product() {   	// Constracter / yapýcý blok
		System.out.println("Ben Çalýþtým");
	}
	
	public Product(int id,String Name, double unitPrice, String detail) {
		this();
		this.id = id;
		this.name = Name;
		this.unitPrice = unitPrice;
		this.Detail = detail;
		
	}
	
	int id ;
	String name ;
	double unitPrice;
	String Detail;
	
	
}
